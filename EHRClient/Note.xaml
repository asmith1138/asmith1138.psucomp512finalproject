<Window x:Class="EHR.Client.Note"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EHR.Client"
        mc:Ignorable="d"
        Title="Note" Height="400" Width="200">
    <Grid>
        <Label Content="Patient Name" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Note By" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Note" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Height="200" Width="150"/>
    </Grid>
</Window>
