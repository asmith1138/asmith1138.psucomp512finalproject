<Window x:Class="EHR.Client.Test"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EHR.Client"
        mc:Ignorable="d"
        Title="Test" Height="400" Width="200">
    <Grid Margin="0,0,0,0">
        <Label Name="PatientName" Content="Patient Name" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Name="OrderedBy" Content="Ordered By" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Name="TestType" Content="Test Type" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Name="Performed" Content="Performed" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Name="Results" Content="Results" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Height="200" Width="150"/>
    </Grid>
</Window>
